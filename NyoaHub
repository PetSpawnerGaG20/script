local lII0O0 = loadstring(game:HttpGet((function()return "https://sirius.menu/rayfield" end)()))()

local O0oo0OO = lII0O0:CreateWindow({
    Name = "ü•äSlap Battlesü•ä",
    LoadingTitle = (function() return "\76\111\97\100\105\110\103..." end)(),
    LoadingSubtitle = (function() return "\66\121\32\83\108\105\118\107\105\110\101\101\112\121" end)(),
})

do
    local IlIlIlI = O0oo0OO:CreateTab("Main")

    IlIlIlI:CreateButton({
        Name = (function() return "\73\110\102\105\110\105\116\101\32\74\117\109\112" end)(),
        Callback = function()
            local OOo0o = game:GetService("Players").LocalPlayer
            game:GetService("UserInputService").JumpRequest:Connect(function()
                pcall(function()
                    if OOo0o.Character and OOo0o.Character:FindFirstChild("Humanoid") then
                        OOo0o.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end)
            end)
        end
    })

    IlIlIlI:CreateSlider({
        Name = "Walkspeed",
        Range = {0, 0x12C},
        Increment = 1,
        CurrentValue = 16,
        Callback = function(v)
            pcall(function()
                local ch = game.Players.LocalPlayer.Character
                if ch and ch:FindFirstChild("Humanoid") then
                    ch.Humanoid.WalkSpeed = v
                end
            end)
        end
    })

    IlIlIlI:CreateInput({
        Name = "Jump Power",
        CurrentValue = "15",
        PlaceholderText = "1-300",
        Callback = function(txt)
            pcall(function()
                local hum = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
                if hum then hum.JumpPower = tonumber(txt) end
            end)
        end
    })

    IlIlIlI:CreateButton({
        Name = "Fly Gui",
        Callback = function()
            loadstring(game:HttpGet((("\104\116\116\112\115\58\47\47").. "raw.githubusercontent.com/PetSpawnerGaG20/script/refs/heads/main/Fly")))()
        end
    })
end

do
    local O0o0 = O0oo0OO:CreateTab("üëÄ Scripts")

    O0o0:CreateButton({
        Name = "AllBadge-Gloves",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/IncognitoScripts/SlapBattles/main/EquipAnyBadgeGloves"))()
        end
    })

    O0o0:CreateButton({
        Name = "Slap-Aura",
        Callback = function()
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title="Slap Aura executed!",
                Text="May not work!",
                Duration=5
            })
            local t=0
            game:GetService("RunService").RenderStepped:Connect(function()
                if tick()-t>0.8 then
                    t=tick()
                    for _,plr in ipairs(game:GetService("Players"):GetPlayers()) do
                        pcall(function()
                            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                                local my = game.Players.LocalPlayer.Character
                                if my and my:FindFirstChild("HumanoidRootPart") and (plr.Character.HumanoidRootPart.Position-my.HumanoidRootPart.Position).Magnitude<10 then
                                    game:GetService("ReplicatedStorage").b:FireServer(plr.Character.HumanoidRootPart)
                                end
                            end
                        end)
                    end
                end
            end)
        end
    })

    O0o0:CreateButton({
        Name = "No Cooldown",
        Callback = function()
            game:GetService("StarterGui"):SetCore("SendNotification",{Title="By Slivkineepy!",Text="No cooldown executed!",Duration=5})
            local pl=game.Players.LocalPlayer
            local ch=pl.Character or pl.CharacterAdded:Wait()
            local tool=ch:FindFirstChildOfClass("Tool") or pl.Backpack:FindFirstChildOfClass("Tool")
            while ch and ch:FindFirstChild("Humanoid") and ch.Humanoid.Health>0 do
                pcall(function()
                    local ls=tool and tool:FindFirstChildOfClass("LocalScript")
                    if ls then
                        local c=ls:Clone()
                        ls:Destroy()
                        c.Parent=tool
                    end
                end)
                task.wait(0.15)
            end
        end
    })
end

do
    local misc = O0oo0OO:CreateTab("‚öôÔ∏è Misc")

    misc:CreateLabel("By NyoaHub | Discord = discord.gg/s65yDmawZx")

    misc:CreateButton({
        Name = "Copy Discord Invite Link",
        Callback = function()
            setclipboard("https://discord.gg/s65yDmawZx")
            game:GetService("StarterGui"):SetCore("SendNotification",{
                Title="Discord Invite Link Copied!",
                Text="Success‚úÖ",
                Duration=3
            })
        end
    })

    misc:CreateButton({
        Name = "CloseGui",
        Callback = function()
            lII0O0:Destroy()
        end
    })
end
