local a=loadstring(game:HttpGet("https://sirius.menu/rayfield"))()
local b=a:CreateWindow({Name="ü•äSlap Battlesü•ä",LoadingTitle="Loading...",LoadingSubtitle="By Slivkineepy"})
do
    local c=b:CreateTab("Main")
    c:CreateButton({Name="Infinite Jump",Callback=function()
        local d=game:GetService("Players").LocalPlayer
        game:GetService("UserInputService").JumpRequest:Connect(function()
            pcall(function()
                if d.Character and d.Character:FindFirstChild("Humanoid")then
                    d.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end)
        end)
    end})
    c:CreateSlider({Name="Walkspeed",Range={0,300},Increment=1,CurrentValue=16,Callback=function(e)
        pcall(function()
            local f=game.Players.LocalPlayer.Character
            if f and f:FindFirstChild("Humanoid")then f.Humanoid.WalkSpeed=e end
        end)
    end})
    c:CreateInput({Name="Jump Power",CurrentValue="15",PlaceholderText="1-300",Callback=function(g)
        pcall(function()
            local h=game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
            if h then h.JumpPower=tonumber(g) end
        end)
    end})
    c:CreateButton({Name="Fly Gui",Callback=function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/PetSpawnerGaG20/script/refs/heads/main/Fly"))()
    end})
end
do
    local i=b:CreateTab("üëÄ Scripts")
    i:CreateButton({Name="AllBadge-Gloves",Callback=function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/IncognitoScripts/SlapBattles/main/EquipAnyBadgeGloves"))()
    end})
    i:CreateButton({Name="No Cooldown",Callback=function()
        game:GetService("StarterGui"):SetCore("SendNotification",{Title="By Slivkineepy!",Text="No cooldown executed!",Duration=5})
        local j=game.Players.LocalPlayer
        local k=j.Character or j.CharacterAdded:Wait()
        local l=k:FindFirstChildOfClass("Tool") or j.Backpack:FindFirstChildOfClass("Tool")
        while k and k:FindFirstChild("Humanoid") and k.Humanoid.Health>0 do
            pcall(function()
                local m=l and l:FindFirstChildOfClass("LocalScript")
                if m then
                    local n=m:Clone()
                    m:Destroy()
                    n.Parent=l
                end
            end)
            task.wait(0.15)
        end
    end})
    local Players=game:GetService("Players")
    local RunService=game:GetService("RunService")
    local ReplicatedStorage=game:GetService("ReplicatedStorage")
    local LocalPlayer=Players.LocalPlayer
    local root
    local function o(p)root=p:WaitForChild("HumanoidRootPart")end
    if LocalPlayer.Character then o(LocalPlayer.Character) end
    LocalPlayer.CharacterAdded:Connect(o)
    local auraEnabled=false
    local auraConnection
    local lastFireTime=0
    local auraRange=13
    local function q(r,a) a:Notify({Title=r,Content=auraEnabled and"Enabled"or"Disabled",Duration=2}) end
    local function s()
        local t,d=nil,math.huge
        if not root then return nil end
        for _,u in ipairs(Players:GetPlayers())do
            if u~=LocalPlayer and u.Character and u.Character:FindFirstChild("HumanoidRootPart")then
                local v=(root.Position-u.Character.HumanoidRootPart.Position).Magnitude
                if v<d and v<=auraRange then t,d=u,v end
            end
        end
        return t
    end
    local function w()
        local x=LocalPlayer.Character
        if not x then return nil end
        for _,y in ipairs(x:GetChildren())do if y:IsA("Tool")then return y end end
        return nil
    end
    local function z(A)
        local B={}
        if ReplicatedStorage:FindFirstChild(A)then
            for _,C in ipairs(ReplicatedStorage[A]:GetChildren())do
                if C:IsA("RemoteEvent")then table.insert(B,C)end
            end
        end
        return B
    end
    i:CreateToggle({Name="Slap Aura",CurrentValue=false,Callback=function(D)
        auraEnabled=D
        q("Slap Aura",a)
        if auraEnabled then
            if auraConnection then auraConnection:Disconnect();auraConnection=nil end
            auraConnection=RunService.Heartbeat:Connect(function()
                local E=os.clock()
                if E-lastFireTime<0.7 then return end
                lastFireTime=E
                if not root then return end
                local F=s()
                if not F or not F.Character then return end
                local G=F.Character:FindFirstChild("HumanoidRootPart")
                if not G then return end
                local H=w()
                local I={}
                if H then local J=z(H.Name) if J and #J>0 then I=J end end
                if #I==0 then for _,K in ipairs({"GeneralHit","b"})do local L=ReplicatedStorage:FindFirstChild(K) if L then table.insert(I,L)end end end
                for _,M in ipairs(I)do M:FireServer(G)end
            end)
        else
            if auraConnection then auraConnection:Disconnect();auraConnection=nil end
        end
    end})
end
do
    local N=b:CreateTab("‚öôÔ∏è Misc")
    N:CreateLabel("By NyoaHub | Discord = discord.gg/s65yDmawZx")
    N:CreateButton({Name="Copy Discord Invite Link",Callback=function()
        setclipboard("https://discord.gg/s65yDmawZx")
        game:GetService("StarterGui"):SetCore("SendNotification",{Title="Discord Invite Link Copied!",Text="Success‚úÖ",Duration=3})
    end})
    N:CreateButton({Name="CloseGui",Callback=function()a:Destroy()end})
end
