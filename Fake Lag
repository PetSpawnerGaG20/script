gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(85, 170, 255)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 102, 204))
}
gradient.Rotation = 45
gradient.Parent = frame

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1,0,0,30)
title.Position = UDim2.new(0,0,0,0)
title.Text = "NyoaHub Fake Lag Script"
title.Font = Enum.Font.GothamBlack
title.TextSize = 16
title.TextColor3 = Color3.fromRGB(170, 200, 255)
title.BackgroundTransparency = 1
title.Parent = frame

spawn(function()
    while wait(0.05) do
        local hue = (tick() % 5)/5
        title.TextColor3 = Color3.fromHSV(hue * 0.3 + 0.5, 0.7, 1)
    end
end)

