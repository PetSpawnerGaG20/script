local box = Instance.new("TextBox")
box.Size = UDim2.new(0.9,0,0,25)
box.Position = UDim2.new(0.05,0,0,80)
box.PlaceholderText = "0 - 10"
box.Text = "0 - 10"
box.BackgroundColor3 = Color3.fromRGB(45,45,70)
box.TextColor3 = Color3.fromRGB(200,220,255)
box.Font = Enum.Font.Gotham
box.TextSize = 13
box.Parent = frame

local boxCorner = Instance.new("UICorner")
boxCorner.CornerRadius = UDim.new(0,6)
boxCorner.Parent = box

local function startFakeLag()
    fakeLag = true
    toggleBtn.Text = "Fake Lag: ON"
    delayValue = tonumber(box.Text) or 1
    spawn(function()
        while fakeLag do
            if char:FindFirstChild("HumanoidRootPart") then
                char.HumanoidRootPart.Anchored = true
                wait(delayValue)
                char.HumanoidRootPart.Anchored = false
            end
            wait(0.05)
        end
    end)
end

local function stopFakeLag()
    fakeLag = false
    toggleBtn.Text = "Fake Lag: OFF"
    if char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.Anchored = false
    end
end

toggleBtn.MouseButton1Click:Connect(function()
    if fakeLag then
        stopFakeLag()
    else
        startFakeLag()
    end
end)

closeBtn.MouseButton1Click:Connect(function()
    confirmFrame.Visible = true
end)

yesBtn.MouseButton1Click:Connect(function()
    stopFakeLag()
    gui:Destroy()
end)

noBtn.MouseButton1Click:Connect(function()
    confirmFrame.Visible = false
end)

-- Dragging
local dragging = false
local dragInput, startPos, startFramePos
local function update(input)
    local delta = input.Position - startPos
    frame.Position = UDim2.new(
        startFramePos.X.Scale,
        startFramePos.X.Offset + delta.X,
        startFramePos.Y.Scale,
        startFramePos.Y.Offset + delta.Y
    )
end

frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        startPos = input.Position
        startFramePos = frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)
